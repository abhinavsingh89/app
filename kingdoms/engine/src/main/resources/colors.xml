<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:util="http://www.springframework.org/schema/util" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
       http://www.springframework.org/schema/beans/spring-beans-3.2.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.2.xsd">

	<util:constant id="RED"
		static-field="org.concordia.kingdoms.domain.Color.RED" />

	<util:constant id="YELLOW"
		static-field="org.concordia.kingdoms.domain.Color.YELLOW" />

	<util:constant id="BLUE"
		static-field="org.concordia.kingdoms.domain.Color.BLUE" />

	<util:constant id="GREEN"
		static-field="org.concordia.kingdoms.domain.Color.GREEN" />

</beans>
